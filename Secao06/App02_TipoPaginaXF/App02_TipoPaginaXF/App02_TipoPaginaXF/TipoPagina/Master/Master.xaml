<?xml version="1.0" encoding="utf-8" ?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:local="clr-namespace:App02_TipoPaginaXF.TipoPagina.Master"
             x:Class="App02_TipoPaginaXF.TipoPagina.Master.Master">

    <MasterDetailPage.Master>
        <!--Menu-->
        <ContentPage Title="Menu">
            <StackLayout >
                <Button Text="Pagina 1" Clicked="MudarPagina1"/>
                <Button Text="Pagina 2" Clicked="MudarPagina2"/>
                <Button Text="Conteudo" Clicked="MudarPagina3"/>
            </StackLayout>
        </ContentPage>
    </MasterDetailPage.Master>

    <MasterDetailPage.Detail>
        <!--Conteudo-->
        <local:Conteudo />
    </MasterDetailPage.Detail>
    
</MasterDetailPage>